APP_CODE_PATH_HOST=../
APP_CODE_PATH_CONTAINER=/usr

VOLUMES_DRIVER=local
NETWORKS_DRIVER=bridge

# Docker Sync
# You may add flags to the path `:cached`, `:delegated`. When using Docker Sync add `:nocopy`
APP_CODE_CONTAINER_FLAG=:cached

DATA_PATH_HOST=~/.js/data

### Docker compose files ##################################

# Select which docker-compose files to include. If using docker-sync append `:docker-compose.sync.yml` at the end
COMPOSE_FILE=docker-compose.yml

WORKSPACE_TIMEZONE=Africa/Lusaka

# Change the separator from : to ; on Windows
COMPOSE_PATH_SEPARATOR=:

# Define the prefix of container names. This is useful if you have multiple projects that use laradock to have seperate containers per project.
COMPOSE_PROJECT_NAME=local

### Docker Sync ###########################################

# If you are using Docker Sync. For `osx` use 'native_osx', for `windows` use 'unison', for `linux` docker-sync is not required
DOCKER_SYNC_STRATEGY=native_osx

### TRAEFIK BACKENDS, URLS & PORTS #################################################
TB_APP=frontend
APP_HOST_URL=app.local.test
APP_CONTAINER_PORT=3000

TB_GRAPHQL=graphql-api
GRAPHQL_API_HOST_URL=graphql.local.test
GRAPHQL_CONTAINER_PORT=4000

TB_PRISMA=prisma
PRISMA_HOST_URL=prisma.local.test
PRISMA_CONTAINER_PORT=4466

TB_STRAPI=strapi
STRAPI_HOST_URL=strapi.local.test
STRAPI_CONTAINER_PORT=1337

TB_MONGO_EXPRESS='mongo-express'
MONGO_EXPRESS_HOST_URL=mongo.local.test
MONGO_EXPRESS_CONTAINER_PORT=8081

TB_JENKINS=jenkins
JENKINS_HOST_URL=jenkins.local.test
JENKINS_CONTAINER_PORT=8080

TB_WEBHOOK=webhook
WEBHOOK_HOST_URL=webhook.local.test
WEBHOOK_CONTAINER_PORT=9000

TB_ADMINER=adminer
ADMINER_HOST_URL=adminer.local.test
ADMINER_CONTAINER_PORT=8080

TB_REDIS_WEBUI=redis
REDIS_WEBUI_HOST_URL=redis.local.test
REDIS_WEBUI_CONTAINER_PORT=80

TB_TRAEFIK=monitor
TRAEFIK_HOST_URL=monitor.local.test

TB_AMBASSADOR=ambassador
AMBASSADOR_HOST_URL=apigateway.local.test
AMBASSADOR_HOST_URL_PREFIX='/api'
AMBASSADOR_CONTAINER_PORT=8080

TB_ELASTICSEARCH=elastic
