{
  "method": "POST",
  "url": "/api/email/template",
  "body": {
    "emailTemplate": {
      "defaultFromName": "#{defaultEmailFromName}",
      "defaultSubject": "Reset your Password",
      "defaultHtmlTemplate": "@{examples/emails/change-password.html.ftl}",
      "defaultTextTemplate": "@{examples/emails/change-password.txt.ftl}",
      "fromEmail": "#{defaultEmailFrom}",
      "name": "Forgot Password New"
    }
  }
}
