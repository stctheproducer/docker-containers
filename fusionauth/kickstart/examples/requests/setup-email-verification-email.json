{
  "method": "POST",
  "url": "/api/email/template",
  "body": {
    "emailTemplate": {
      "defaultFromName": "#{defaultEmailFromName}",
      "defaultSubject": "Verify your Email Address",
      "defaultHtmlTemplate": "@{examples/emails/email-verification.html.ftl}",
      "defaultTextTemplate": "@{examples/emails/email-verification.txt.ftl}",
      "fromEmail": "#{defaultEmailFrom}",
      "name": "Email Verification New"
    }
  }
}
