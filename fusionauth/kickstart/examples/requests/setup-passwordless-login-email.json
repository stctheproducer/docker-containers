{
  "method": "POST",
  "url": "/api/email/template",
  "body": {
    "emailTemplate": {
      "defaultFromName": "#{defaultEmailFromName}",
      "defaultSubject": "Log into Company Name",
      "defaultHtmlTemplate": "@{examples/emails/passwordless-login.html.ftl}",
      "defaultTextTemplate": "@{examples/emails/passwordless-login.txt.ftl}",
      "fromEmail": "#{defaultEmailFrom}",
      "name": "Passwordless Login New"
    }
  }
}
