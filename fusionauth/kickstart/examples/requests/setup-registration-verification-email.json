{
  "method": "POST",
  "url": "/api/email/template",
  "body": {
    "emailTemplate": {
      "defaultFromName": "#{defaultEmailFromName}",
      "defaultSubject": "Verify your Registration",
      "defaultHtmlTemplate": "@{examples/emails/registration-verification.html.ftl}",
      "defaultTextTemplate": "@{examples/emails/registration-verification.txt.ftl}",
      "fromEmail": "#{defaultEmailFrom}",
      "name": "Registration Verification New"
    }
  }
}
